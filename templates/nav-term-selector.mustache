<div class="dropdown m-b-1 ml-1 mr-auto">
    <div id="termdropdown" hidden>
    <button type="button" class="btn btn-outline-secondary dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" aria-label="{{#str}} aria:groupingdropdown, block_myoverview_term_filter {{/str}}">
        <span class="d-sm-inline-block" data-active-item-text>
            {{#all}}{{#str}} all, block_myoverview_term_filter {{/str}}{{/all}}
            {{#inprogress}}{{#str}} inprogress, block_myoverview_term_filter {{/str}}{{/inprogress}}
            {{#future}}{{#str}} future, block_myoverview_term_filter {{/str}}{{/future}}
            {{#past}}{{#str}} past, block_myoverview_term_filter {{/str}}{{/past}}
            {{#favourites}}{{#str}} favourites, block_myoverview_term_filter {{/str}}{{/favourites}}
            {{#hidden}}{{#str}} hiddencourses, block_myoverview_term_filter {{/str}}{{/hidden}}
            {{#semesterweise}}{{#str}} semestercourses, block_myoverview_term_filter {{/str}}{{/semesterweise}}
        </span>
    </button>
    <ul class="dropdown-menu multilevel" data-show-active-item data-active-item-text role="menu"  aria-labelledby="groupingdropdown">
        <li>
            <a class="dropdown-item {{#all}}active{{/all}}" href="#" data-filter="grouping" data-value="all" data-pref="all" aria-label="{{#str}} aria:allcourses, block_myoverview_term_filter {{/str}}" aria-controls="courses-view-{{uniqid}}">
                {{#str}} all, block_myoverview_term_filter {{/str}}
            </a>
        </li>
        <li>
            <a class="dropdown-item {{#inprogress}}active{{/inprogress}}" href="#" data-filter="grouping" data-value="inprogress" data-pref="inprogress" aria-label="{{#str}} aria:inprogress, block_myoverview_term_filter {{/str}}" aria-controls="courses-view-{{uniqid}}">
                {{#str}} inprogress, block_myoverview_term_filter {{/str}}
            </a>
        </li>
        <li>
            <a class="dropdown-item {{#future}}active{{/future}}" href="#" data-filter="grouping" data-value="future" data-pref="future" aria-label="{{#str}} aria:future, block_myoverview_term_filter {{/str}}" aria-controls="courses-view-{{uniqid}}">
                {{#str}} future, block_myoverview_term_filter {{/str}}
            </a>
        </li>
        <li>
            <a class="dropdown-item {{#past}}active{{/past}}" href="#" data-filter="grouping" data-value="past" data-pref="past" aria-label="{{#str}} aria:past, block_myoverview_term_filter {{/str}}" aria-controls="courses-view-{{uniqid}}">
                {{#str}} past, block_myoverview_term_filter {{/str}}
            </a>
        </li>
        <li>
            <a class="dropdown-item {{#favourites}}active{{/favourites}}" href="#" data-filter="grouping" data-value="favourites"  data-pref="favourites" aria-label="{{#str}} aria:favourites, block_myoverview_term_filter {{/str}}" aria-controls="courses-view-{{uniqid}}">
                {{#str}} favourites, block_myoverview_term_filter {{/str}}
            </a>
        </li>
        <li>
            <a class="dropdown-item {{#hidden}}active{{/hidden}}" href="#" data-filter="grouping" data-value="hidden"  data-pref="hidden" aria-label="{{#str}} aria:hiddencourses, block_myoverview_term_filter {{/str}}" aria-controls="courses-view-{{uniqid}}">
                {{#str}} hiddencourses, block_myoverview_term_filter {{/str}}
            </a>
        </li>
        <li>
            <a class="dropdown-item" id="semester-dropdown-option" href="#" data-filter="grouping" data-value="term"  data-pref="term" aria-label="semesterkurse" aria-controls="courses-view-{{uniqid}}">
                {{#str}} semestercourses, block_myoverview_term_filter {{/str}}
            </a>
        </li>
    </ul>
    </div>
</div>